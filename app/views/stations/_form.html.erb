<%= render("layouts/errors", :object => @station) %>

<%= form_for(@station) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <%= f.submit(:class => "btn btn-primary") %>
<% end %>
<br>
<%= link_to "Return Home", stations_path, :class => "btn btn-danger" %>
